{
  "name": "@typographist/core",
  "version": "26.0.5",
  "description": "Core API for typographist",
  "license": "MIT",
  "homepage": "https://github.com/mg901/typographist#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mg901/typographist.git"
  },
  "bugs": {
    "url": "https://github.com/mg901/typographist/issues"
  },
  "author": "mg901 <mgreen097@gmail.com>",
  "keywords": [
    "css-in-js",
    "design",
    "golden-ratio",
    "modular",
    "modular-scale",
    "musical-scales",
    "ratio",
    "scale",
    "typography",
    "typographist",
    "web-typography"
  ],
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "build": "yarn clean && tsc && yarn size",
    "clean": "../../node_modules/.bin/rimraf build",
    "size": " size-limit",
    "commit": "../../node_modules/.bin/git-cz",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "pretty": "../../node_modules/.bin/prettier --write '**/*.ts'",
    "pretty:watch": "../../node_modules/.bin/onchange '**/*.{j,t}s' -- prettier --write {{changed}}",
    "lint:file": "../../node_modules/.bin/eslint --fix",
    "lint:all": "../../node_modules/.bin/eslint --fix .",
    "test": "../../node_modules/.bin/jest --colors",
    "test:watch": "../../node_modules/.bin/jest --colors --watch"
  },
  "publishConfig": {
    "access": "public"
  }
}
