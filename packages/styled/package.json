{
  "name": "@typographist/styled",
  "version": "2.1.4-alpha.1",
  "license": "MIT",
  "homepage": "https://github.com/mg901/typographist",
  "bugs": "https://github.com/mg901/typographist/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mg901/typographist.git"
  },
  "contributors": [
    "Maxim Alyoshin <mgreen097@gmail.com>",
    "Sergey Sova <mail@sergeysova.com>"
  ],
  "keywords": [
    "css-in-js",
    "design",
    "golden-ratio",
    "modular",
    "modular-scale",
    "musical-scales",
    "ratio",
    "scale",
    "typography",
    "typographist",
    "web-typography"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn test && yarn lint:all && yarn ts-check && tsc && yarn size",
    "clean": "cross-env fixme=fixme ../../node_modules/.bin/rimraf build",
    "size": " cross-env size-limit",
    "commit": "cross-env fixme=fixme ../../node_modules/.bin/git-cz",
    "coverage": "cross-env fixme=fixme cat ./coverage/lcov.info | coveralls",
    "pretty": "cross-env fixme=fixme ../../node_modules/.bin/prettier --write '**/*.ts'",
    "pretty:watch": "cross-env fixme=fixme ../../node_modules/.bin/onchange '**/*.{j,t}s' -- prettier --write {{changed}}",
    "ts-check": "cross-env fixme=fixme ../../node_modules/.bin/tsc --noEmit",
    "ts-check:watch": "yarn ts-check -- --watch",
    "lint:file": "cross-env fixme=fixme ../../node_modules/.bin/eslint --fix",
    "lint:all": "cross-env fixme=fixme ../../node_modules/.bin/eslint --fix .",
    "test": "cross-env fixme=fixme ../../node_modules/.bin/jest",
    "test:watch": "cross-env fixme=fixme ../../node_modules/.bin/jest --watch"
  },
  "files": [
    "build"
  ],
  "peerDependencies": {
    "react": "^16.x.x",
    "react-dom": "^16.x.x",
    "styled-components": "^4.x.x"
  },
  "dependencies": {
    "@typographist/core": "^25.1.1-rc",
    "@typographist/modular-scale": "1.1.3-rc",
    "memoize-one": "^5.1.1",
    "styled-breakpoints": "^7.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.2.1",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
