{
  "name": "@typographist/utils",
  "version": "2.0.5",
  "license": "MIT",
  "scripts": {
    "size": " cross-env size-limit",
    "commit": "cross-env ../../node_modules/.bin/git-cz",
    "coverage": "cross-env cat ./coverage/lcov.info | coveralls",
    "pretty": "cross-env ../../node_modules/.bin/prettier --write '**/*.ts'",
    "pretty:watch": "cross-env  ./../node_modules/.bin/onchange '**/*.{j,t}s' -- prettier --write {{changed}}",
    "lint:file": "cross-env ../../node_modules/.bin/eslint --fix",
    "lint:all": "cross-env ../../node_modules/.bin/eslint --fix ."
  },
  "publishConfig": {
    "access": "public"
  }
}
